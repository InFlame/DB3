<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="media.definitions">
	<class name="Category" table="Kategorie">
		<id column="Namen" name="name" type="string">
			<generator class="native" />
		</id>
		<!-- <composite-id mapped="false" unsaved-value="undefined"> <key-property 
			column="Namen" name="name" type="string" /> <key-property column="Parent" 
			name="parent" type="string" /> </composite-id> -->
		<many-to-one class="media.definitions.Category" name="parent"
			insert="false" update="false" column="parent">
		</many-to-one>
		<property name="parent" column="parent" />
	</class>
</hibernate-mapping>