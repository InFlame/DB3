<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="media.definitions">
	<class name="Category" table="Kategorie">
		<composite-id mapped="false" unsaved-value="undefined">
			<key-property column="Namen" name="name" type="string" />
			<key-property column="Parent" name="parent" type="string" />
		</composite-id>
		<many-to-one class="media.definitions.Category"
			name="parent" insert="false" update="false">
			<column name="Namen" />
			<column name="Parent" />
		</many-to-one>
		<!-- <set name="Namen" table="Category" lazy="false" cascade="all-delete-orphan"
			inverse="false">
			<key column="Parent" />
			<one-to-many class="media.definitions.Category" />
		</set> -->

	</class>
</hibernate-mapping>
