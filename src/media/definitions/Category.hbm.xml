<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="media.definitions">
<class table="Kategorie" name="Category">
	<composite-id>
		<key-property name="name" type="string" column="Namen"/>
		<key-property name="parent" type="string" column="Parent"/>
	</composite-id>
</class>
</hibernate-mapping>